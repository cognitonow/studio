
// Use FDL to specify the queries and mutations that the connector will expose.
// Each query and mutation has a unique name and is associated with a function
// from the schema.
//
// See: https://firebase.google.com/docs/data-connect/reference/fdl/connector
connector default {
  import schema "../schema.fbs";

  // Expose the listServices function as a query.
  query listServices: schema.listServices;
  query listUsers: schema.listUsers;
  mutation insertUser: schema.insertUser;
}
